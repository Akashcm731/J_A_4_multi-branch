pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'checking-out at devlopment branch'
            }
        }
        stage('test') {
            steps {
                echo 'running the test at devlopment branch'
            }
        }
        stage('build') {
            steps {
                echo 'building the code at devlopment branch'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying from devlopment branch'
            }
        }
    }
}
